{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/:path*.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot)",
      "destination": "/:path*"
    },
    {
      "source": "/preview/:id",
      "destination": "/api/ssrOG"
    },
    {
      "source": "/(.*)",
      "destination": "/api/ssrOG"
    }
  ],
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1600, 1920, 2048],
    "domains": ["alxproduction.blob.core.windows.net"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "alxproduction.blob.core.windows.net",
        "pathname": "/multipost-public/**"
      }
    ]
  }
}
